{
    "workbench.iconTheme": "vscode-icons",
    "editor.fontSize": 16.5,
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        "c": "cd $dir ; clang \"$fileName\" --target=x86_64-w64-mingw32 -o \"$workspaceRoot\\a\" -Wall -std=c11 -g ; if ($?) { .\"$workspaceRoot\\a\" }",
        "cpp": "cd $dir ; clang++ \"$fileName\" --target=x86_64-w64-mingw32 -o \"$workspaceRoot\\a\" -Wall -std=c++14 -g ; if ($?) { .\"$workspaceRoot\\a\" }"
    },
    "code-runner.clearPreviousOutput": false,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.ignoreSelection": true,
    "code-runner.preserveFocus": false,
    "workbench.colorTheme": "Winter is Coming (Dark Blue - No Italics)",
    "editor.fontFamily": "'Cascadia Mono PL', 'Ubuntu Mono', Consolas, 'Microsoft Yahei'",
    "editor.suggestSelection": "first",
    "files.exclude": {
        "_": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/*.exe": true,
        "**/a": true
    },
    "python.languageServer": "Jedi",
    "files.defaultLanguage": "${activeEditorLanguage}",
    "sync.gist": "a71982b19214ce7915d426285279c0a3",
    "C_Cpp.updateChannel": "Insiders",
    "editor.minimap.scale": 3,
    "window.zoomLevel": 1,
    "http.proxy": "http://127.0.0.1:7890",
    "editor.tabSize": 2,
    "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7-preview\\pwsh.exe",
    "files.associations": {
        "*.txt": "plaintext",
        "typeinfo": "cpp",
        "any": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "cfenv": "cpp",
        "charconv": "cpp",
        "chrono": "cpp",
        "cinttypes": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "codecvt": "cpp",
        "complex": "cpp",
        "condition_variable": "cpp",
        "csetjmp": "cpp",
        "csignal": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cuchar": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "ratio": "cpp",
        "regex": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "fstream": "cpp",
        "future": "cpp",
        "initializer_list": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "scoped_allocator": "cpp",
        "shared_mutex": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "thread": "cpp",
        "typeindex": "cpp",
        "valarray": "cpp",
        "variant": "cpp",
        "ccomplex": "cpp"
    },
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "C_Cpp.intelliSenseEngine": "Disabled",
    "clangd.arguments": [
        "--clang-tidy",
        "--pch-storage=memory",
        "--enable-config",
        "--header-insertion=never"
    ],
    "http.proxyStrictSSL": false
}
